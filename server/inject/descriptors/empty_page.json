{
  "type": "object",
  "properties": {
    "isEmptyPage": {
      "type": "identifier",
      "selector": ":not(iframe)",
      "control": "exists"
    },
    "goToMain": {
      "type": "action",
      "selector": "html",
      "control": "url.login"
    }
  },
  "connect": [
    {
      "match": [
        "isEmptyPage"
      ],
      "action": "goToMain"
    }
  ]
}

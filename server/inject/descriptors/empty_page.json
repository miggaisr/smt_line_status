{
  "type": "object",
  "properties": {
    "isEmptyPage": {
      "type": "identifier",
      "control": "exists"
    },
    "goToMain": {
      "type": "action",
      "selector": {
        "fn": "location.href='main.php';",
        "selector": ":not(url.main)"
      }
    }
  },
  "connect": [
    {
      "match": [
        "isEmptyPage"
      ],
      "action": "goToMain"
    }
  ]
}

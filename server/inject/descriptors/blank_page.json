{
  "type": "object",
  "properties": {
    "isBlankPage": {
      "type": "identifier",
      "control": "url.login",
      "selector": {
        "fn": ":not(div)",
        "selector": "html"
      }
    }
  },
  "connect": [
    {
      "match": [
        "isBlankPage"
      ],
      "event": "goToMain"
    }
  ]
}

{
  "type": "object",
  "properties": {
    "isBlankPage": {
      "type": "identifier",
      "selector": "head > script",
      "control": "exists",
      "selectContains": "json_call( 'php_class=general_functions&func=login' )"
    }
  },
  "connect": [
    {
      "match": [
        "isBlankPage"
      ],
      "event": "goToMain"
    }
  ]
}

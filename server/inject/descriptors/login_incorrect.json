{
  "type": "object",
  "properties": {
    "isLoginIncorrect": {
      "type": "identifier",
      "selector": ".h1_m",
      "control": "exists",
      "selectContains": "Please contact"
    },
    "LogInAgain": {
      "type": "action",
      "selector": ".orange_button",
      "control": "action.button"
    }
  },
  "connect": [
    {
      "match": [
        "isLoginIncorrect"
      ],
      "action": "LogInAgain"
    }
  ]
}

{
  "type": "object",
  "properties": {
    "isSmtLinesList": {
      "type": "identifier",
      "selector": "html",
      "control": "url.show_smt_lines"
    },
    "lines_list": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "line": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText"
          },
          "paka": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 1
          },
          "pn": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 2
          },
          "side": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 4
          },
          "qty": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 5
          },
          "qty_of_cards": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 6
          },
          "wo_in_baan": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 7
          },
          "pn_description": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 3
          },
          "code_description": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 8
          },
          "side_start_time": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 9
          },
          "code_duration": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 10
          },
          "last_side": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 11
          },
          "color": {
            "type": "string",
            "control": "exists",
            "selector": "td"
          }
        }
      },
      "selectorAll": "tr"
    },
    "header": {
      "type": "string",
      "selector": ".h1_m",
      "control": "property.innerHTML",
      "selectContains": "SMT lines status",
      "selectIndex": 15
    }
  },
  "connect": [
    {
      "match": [
        "isSmtLinesList"
      ],
      "screens": [
        "smt_lines_list"
      ]
    }
  ]
}

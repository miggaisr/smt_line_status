{
  "type": "object",
  "properties": {
    "goToReports": {
      "type": "action",
      "selector": ".main_menu_header",
      "control": "action.click",
      "selectContains": "Reports"
    },
    "isMain": {
      "type": "identifier",
      "selector": "#main_layer",
      "control": "exists"
    },
    "go_to_smt_main_reports": {
      "type": "action",
      "selector": ".menu_buttons",
      "control": "action.link",
      "selectContains": "SMT main reports"
    },
    "isReports": {
      "type": "identifier",
      "selector": ".menu_buttons",
      "control": "exists",
      "selectContains": "SMT main reports"
    },
    "isSMTMainReports": {
      "type": "identifier",
      "selector": ".menu_buttons",
      "control": "exists",
      "selectContains": "SMT lines status"
    },
    "goToLinesReport": {
      "type": "action",
      "selector": "div.top_menu_buttons",
      "control": "action.click",
      "selectContains": "SMT lines status"
    },
    "reportOpen": {
      "type": "identifier",
      "selector": ".menu_buttons",
      "control": "exists",
      "selectContains": "SMT main reports"
    }
  },
  "connect": [
    {
      "match": [
        "isMain"
      ],
      "action": "goToReports"
    },
    {
      "match": [
        "isReports"
      ],
      "action": "go_to_smt_main_reports"
    },
    {
      "match": [
        "isSMTMainReports"
      ],
      "action": "goToLinesReport"
    }
  ]
}

{
  "type": "object",
  "properties": {
    "goToReports": {
      "type": "action",
      "selector": ".main_menu_header",
      "control": "action.click",
      "selectContains": "Reports"
    },
    "isMain": {
      "type": "identifier",
      "selector": "#main_layer",
      "control": "exists"
    }
  },
  "connect": [
    {
      "match": [
        "isMain"
      ],
      "action": "goToReports"
    }
  ],
  "selector": ".vsdg"
}

{
  "type": "object",
  "properties": {
    "isSmtMainReports": {
      "type": "identifier",
      "selector": ".menu_buttons",
      "control": "exists",
      "selectContains": "SMT lines status"
    },
    "GoToLineStatus": {
      "type": "action",
      "control": {
        "fn": "top.main.location.href='process_engineering/lines_report.php'; ;top.jQuery('#submenu').hide()",
        "control": "exists"
      },
      "selector": "top_menu_buttons orange_button"
    }
  },
  "connect": [
    {
      "match": [
        "isSmtMainReports"
      ],
      "action": "GoToLineStatus"
    }
  ]
}

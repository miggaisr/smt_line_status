{
  "type": "object",
  "properties": {
    "isMain": {
      "type": "identifier",
      "control": "url.main",
      "selector": "html"
    },
    "GoToSmtMainReports": {
      "type": "action",
      "control": "action.link",
      "selector": ".menu_buttons",
      "selectContains": "SMT main reports"
    }
  },
  "connect": [
    {
      "match": [
        "isMain"
      ],
      "action": "GoToSmtMainReports"
    }
  ]
}

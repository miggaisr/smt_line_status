{
  "type": "object",
  "properties": {
    "isMain": {
      "type": "identifier",
      "control": "exists",
      "selector": "href='main.php'"
    },
    "GoToSmtMainReports": {
      "type": "action",
      "control": "action.link",
      "selector": ".menu_buttons",
      "selectContains": "SMT main reports"
    }
  },
  "connect": [
    {
      "match": [
        "isMain"
      ],
      "action": "GoToSmtMainReports"
    }
  ]
}
